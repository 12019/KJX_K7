 469:../../MMI/Resource/ABMLoader.c ****         *bits_mem_ptr++ = bits_buf;                             \
 470:../../MMI/Resource/ABMLoader.c ****         bits_buf >>= 16;                                        \
 471:../../MMI/Resource/ABMLoader.c ****         bits_left -= 16;                                        \
 472:../../MMI/Resource/ABMLoader.c ****     }
